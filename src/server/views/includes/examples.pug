      input(type='text',class='form-control quicksearch',placeholder='Search')
//- spinner
.spinner
  .rect1
  .rect2
  .rect3
  .rect4
  .rect5


script.
  let loadedImages = 0;
#examples-grid.clearfix.hidden
  .grid-sizer.col-xs-12.col-sm-4.col-md-3.col-lg-2
  each eg in examples
    a(class="wrapper-link",href= env.EDITOR_ADDRESS+'/?action=example&ProjectName='+eg.projectName + '&editMode=true', target='_blank') 
      .element-item.col-xs-12.col-sm-4.col-md-3.col-lg-2
        .h-thumbnail
          .img-container
            img.img-responsive.center-block.img-thumbnail(src= `${env.EDITOR_ADDRESS}/api/examples/${eg.projectName}/thumbnail?aspectRatio=1.33333`, onload="loadedImages++", style='width: 100%;')
            .overlay
              .caption.text-center= eg.notes || 'No description provided.'
          ul.list-group
            span.label.label-success= eg.services[0]
            li.list-group-item.title
              b(title= eg.projectName)= eg.projectName
              if eg.roleNames.length > 1
                i.fa.fa-users.pull-right(title="Multiplayer")
